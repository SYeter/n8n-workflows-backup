 orderedActual = orderJsonKeys(n8nWorkflow);\n  \n  // Proper formatting\n  $input.all()[0].json.github_status = \"new\";\n  $input.all()[0].json.n8n_data_stringy = JSON.stringify(orderedActual, null, 2);\n}\n\n// Return items\nreturn $input.all();\n"},"id":"212df282-8f58-4fa9-b840-dbdbb0928fb6","name":"isDiffOrNew","type":"n8n-nodes-base.code","position":[528,688],"typeVersion":1},{"parameters":{"dataType":"string","value1":"={{$json.github_status}}","rules":{"rules":[{"value2":"same"},{"value2":"different","output":1},{"value2":"new","output":2}]}},"id":"2503c83e-b7a3-4ba9-95e9-f10706da544c","name":"Check Status","type":"n8n-nodes-base.switch","position":[928,688],"typeVersion":1},{"parameters":{},"id":"a8e4ff81-0a22-4fba-a5d6-63a37dbd8314","name":"Same file - Do nothing","type":"n8n-nodes-base.noOp","position":[1152,528],"typeVersion":1},{"parameters":{},"id":"d5435589-4b1b-4693-8ec5-e3abe8bbd456","name":"File is different","type":"n8n-nodes-base.noOp","position":[1152,688],"typeVersion":1},{"parameters":{},"id":"d1a1e0fc-edcc-4fcf-83f4-893f980dad8b","name":"File is new","type":"n8n-nodes-base.noOp","position":[1152,880],"typeVersion":1},{"parameters":{"resource":"file","owner":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_owner }}"},"repository":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_name }}"},"filePath":"={{ $('Config').first().item.repo_path }}{{ $json.subPath }}{{$('Execute Workflow Trigger').first().json.id}}.json","fileContent":"={{$('isDiffOrNew').item.json[\"n8n_data_stringy\"]}}","commitMessage":"={{$('Execute Workflow Trigger').first().json.name}} ({{$json.github_status}})"},"id":"3b2943db-6862-454e-b51c-7f70d36bcd2f","name":"Create new file","type":"n8n-nodes-base.github","position":[1376,880],"typeVersion":1,"webhookId":"c2632857-7b76-4aaa-9875-9a8afcff3fbc"},{"parameters":{"resource":"file","operation":"edit","owner":{"__rl":true,"mode":"","value":"={{ $('Config').first().item.repo_owner }}"},"repository